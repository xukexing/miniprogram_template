
/* 全局样式 */
page{
  background: #fff;
  padding-bottom: env(safe-area-inset-bottom);
  min-height: 100%;
  -webkit-overflow-scrolling: touch;
}
::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}
/* 授权 */
.authorizationWarp{
  position: fixed;
  width: 100%;
  height: 100%;
  left: 0;
  top: 0;
  z-index: 9999;
  background: rgba(0, 0, 0, 0.7);
  display: flex;
  align-items: center;
  justify-content: center
}
image{
  margin: 0;
  margin-bottom: 0 !important;
  flex-shrink: 0;/*防止被压缩*/
  -webkit-flex-shrink:0
}
button{
  font-weight: normal !important;
  padding: 0;
  margin: 0;
  border-radius: none;
  outline: none;
  background-color: transparent;
}

/* 背景图 */
.bgReset{
  background-position: center;
  background-size: 100%;
  background-repeat: no-repeat;
}
/* 一行显示超出变省略号 */
.oneEllipsis{
  white-space:nowrap;
  overflow:hidden;
  text-overflow:ellipsis;
}
.twoEllipsis{
  display: -webkit-box;
  overflow: hidden;
  text-overflow: ellipsis;
  word-wrap: break-word;
  white-space: normal !important;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}
.threeEllipsis{
  display: -webkit-box;
  overflow: hidden;
  text-overflow: ellipsis;
  word-wrap: break-word;
  white-space: normal !important;
  -webkit-line-clamp: 3;
  -webkit-box-orient: vertical;
}
/* nextIcon */
.nextIcon{
  width: 12rpx;
  height: 18rpx;
  margin-left: 11rpx;
}
.downIcon{
  width:24rpx;
  height:24rpx;
}
.upIcon{
  width:24rpx;
  height:24rpx;
}
/* 开关 */
.wx-switch-input {
  width: 100rpx !important;
  height: 50rpx !important;
  border: none !important;
  box-sizing: border-box !important;
  padding: 5rpx 6rpx!important;
}
.wx-switch-input::before {
  width: 100rpx !important;
  height: 50rpx !important;
  background: #E2E2E2 !important;
  border: none !important; 
}
.wx-switch-input::after {
  width: 40rpx !important;
  height: 40rpx !important;
  margin-left: 10rpx;
  margin-top: 5rpx;
}
